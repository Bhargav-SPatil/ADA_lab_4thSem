#include <stdio.h>

int main()
{
    int n, i, pos, flag = 0;
    int num[] = {40, 30, 60, 10, 80, 50, 70, 100, 20, 90};
    printf("Enter the number to search: ");
    scanf("%d", &n);
    
    for(i = 0; i < 10; i++){
        if(num[i] == n){
            pos = i;
            flag = 1;
            break;
        }
    }
    
    if(flag == 0)
    printf("Element not found in the array \n");
    else
    printf("Element found in the array at pos: %d", pos + 1);
    
    
    return 0;
}
