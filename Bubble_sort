#include <stdio.h>

int main()
{
    int n = 10, i, j, temp, flag = 0;
    int num[] = {40, 30, 60, 10, 80, 50, 70, 100, 20, 90};
    
    for(i = 0; i < 9; i++){
        for(j = 0; j < n - i - 1; j++){
            if(num[j] > num[j + 1]){
                temp = num[j];
                num[j] = num[j+1];
                num[j+1] = temp;
            }
        }
    }
    
    printf("Sorted array is: \n");
    for(i = 0; i < n; i++)
    printf("%d \t", num[i]);
    
    
    return 0;
}
